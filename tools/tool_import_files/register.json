    "dd1362": {
      "inf": "Implementación [component_text_area]",
      "dato": {
        "lg-nolan": [
          "<p>This tool does not appear in all the selected components, only in the correctly configured ones (by configuring the ontology properties).</p><p>&nbsp;</p><p>To activate this tool, set a component_portal Ontology properties to configure it.</p><p>Sample from component oh17 (section oh1 - Oral History)</p><p>&nbsp;</p><p>\"tool_config\": {</p><p>&nbsp; &nbsp;\"tool_import_files\": {</p><p>&nbsp; &nbsp; &nbsp;\"ddo_map\": [</p><p>&nbsp; &nbsp; &nbsp; &nbsp;{</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"role\": \"target_component\",</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"tipo\": \"rsc29\",</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"section_id\": \"self\",</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"section_tipo\": \"rsc170\"</p><p>&nbsp; &nbsp; &nbsp; &nbsp;},</p><p>&nbsp; &nbsp; &nbsp; &nbsp;{</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"role\": \"target_date\",</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"tipo\": \"rsc44\",</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"section_id\": \"self\",</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"section_tipo\": \"rsc170\"</p><p>&nbsp; &nbsp; &nbsp; &nbsp;},</p><p>&nbsp; &nbsp; &nbsp; &nbsp;{</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"role\": \"target_filename\",</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"tipo\": \"rsc398\",</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"section_id\": \"self\",</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"section_tipo\": \"rsc170\"</p><p>&nbsp; &nbsp; &nbsp; &nbsp;},</p><p>&nbsp; &nbsp; &nbsp; &nbsp;{</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"mode\": \"edit\",</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"role\": \"input_component\",</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"tipo\": \"rsc52\",</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"section_id\": \"self\",</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"section_tipo\": \"rsc170\"</p><p>&nbsp; &nbsp; &nbsp; &nbsp;},</p><p>&nbsp; &nbsp; &nbsp; &nbsp;{</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"mode\": \"edit\",</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"role\": \"input_component\",</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"tipo\": \"rsc31\",</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"section_id\": \"self\",</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"section_tipo\": \"rsc170\"</p><p>&nbsp; &nbsp; &nbsp; &nbsp;},</p><p>&nbsp; &nbsp; &nbsp; &nbsp;{</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"mode\": \"edit\",</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"role\": \"input_component\",</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"tipo\": \"rsc23\",</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"section_id\": \"self\",</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;\"section_tipo\": \"rsc170\"</p><p>&nbsp; &nbsp; &nbsp; &nbsp;}</p><p>&nbsp; &nbsp; &nbsp;]</p><p>&nbsp; &nbsp;}</p><p>&nbsp;}</p>"
